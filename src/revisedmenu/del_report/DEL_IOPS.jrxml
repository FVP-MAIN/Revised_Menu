	<queryString>
		<![CDATA[select    a.drdate, b.itmcod,sum(b.qty), c.des,c.supnme
from       dr_h2_iops a, dr_d2_iops b, items_iops c
where    a.drno = b.drno
and        b.itmcod = c.itmcod
and       a.isdlvry = 'T'
and       a.drdate >= $P{past}
and        a.drdate <= $P{present}
and       (a.iscancel = 'F' or a.iscancel is null or a.iscancel =  ' ')
and c.supnme in(select supnme from supplier_vita)
group by a.drdate,b.itmcod,c.des,c.supnme
order by c.supnme]]>
	</queryString>
